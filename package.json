{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "build": "nuxt build",
    "build_local": "nuxt build && node .output/server/index.mjs",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "postinstall": "nuxt prepare",
    "lint:fix": "eslint . --fix",
    "clean": "rm -rf node_modules && rm -rf .output && rm -rf .nuxt",
    "precommit": "pnpm lint && pnpm typecheck && pnpm build",
    "db:run": "tsx --tsconfig ./tsconfig.json node_modules/drizzle-kit/bin.cjs",
    "db:generate": "pnpm db:run generate",
    "db:studio": "pnpm db:run studio",
    "db:drop": "pnpm db:run drop",
    "db:migrate": "pnpm db:run migrate",
    "db:push": "pnpm db:run push",
    "db:introspect": "pnpm db:run introspect"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@nuxthub/core": "^0.7.19",
    "@nuxtjs/i18n": "8.5.3",
    "@pinia/nuxt": "^0.5.4",
    "@tanstack/vue-query": "^5.56.2",
    "@trpc/client": "11.0.0-rc.490",
    "@trpc/server": "11.0.0-rc.490",
    "@wisemen/vue-core": "^0.1.5",
    "drizzle-orm": "^0.33.0",
    "formango": "^2.0.36",
    "i18n-iso-countries": "^7.12.0",
    "libphonenumber-js": "^1.11.8",
    "maska": "3.0.2",
    "nuxt": "3.13.2",
    "nuxt-auth-utils": "^0.3.8",
    "nuxt-time": "^1.0.0",
    "nuxt-typed-router": "^3.6.5",
    "pinia": "^2.2.2",
    "postgres": "^3.4.4",
    "radix-vue": "^1.9.5",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1",
    "trpc-nuxt": "0.11.0-beta.1",
    "vue": "3.5.6",
    "vue-router": "4.4.5",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "^0.5.7",
    "@nuxt/image": "1.8.0",
    "@nuxt/test-utils": "3.14.2",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "@types/dom-view-transitions": "^1.0.5",
    "@types/google.maps": "^3.58.0",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "11.1.0",
    "@wisemen/eslint-config-vue": "^0.2.0",
    "better-sqlite3": "^11.3.0",
    "drizzle-kit": "^0.24.2",
    "eslint": "9.10.0",
    "nuxthub": "^0.6.1",
    "sass": "1.79.1",
    "typescript": "5.6.2",
    "vitest": "2.1.1",
    "vue-tsc": "2.1.6",
    "wrangler": "^3.78.4"
  }
}
